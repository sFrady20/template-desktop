    "@unocss/reset": "^0.53.5",
    "clsx": "^2.0.0",
    "electron-updater": "^6.1.1",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.1.0",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "electron": "^25.3.1",
    "electron-builder": "^24.4.0",
    "electron-vite": "^1.0.25",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6",
    "unocss": "^0.53.5",
    "vite": "^4.4.4"
